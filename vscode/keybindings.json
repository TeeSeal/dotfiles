[
  {
    "key": "ctrl+cmd+/",
    "command": "extension.align",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+cmd+a",
    "command": "-extension.align",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+r",
    "command": "workbench.action.gotoSymbol"
  },
  {
    "key": "shift+cmd+o",
    "command": "-workbench.action.gotoSymbol"
  },
  {
    "key": "cmd+k cmd+p",
    "command": "copyRelativeFilePath"
  },
  {
    "key": "shift+alt+cmd+c",
    "command": "-copyRelativeFilePath",
    "when": "!editorFocus"
  },
  {
    "key": "shift+cmd+b",
    "command": "workbench.action.tasks.runTask"
  },
  {
    "key": "alt+cmd+b",
    "command": "workbench.action.tasks.build"
  },
  {
    "key": "shift+cmd+b",
    "command": "-workbench.action.tasks.build"
  }
]
